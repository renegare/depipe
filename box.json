{
    "directories": ["src"],
    "chmod": "0755",
    "main": "app",
    "output": "depipe.phar",
    "stub": true,
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests", "Test"],
            "in": "vendor"
        },
        {
            "name": "*.pem",
            "in": "vendor"
        },
        {
            "name": "*.pem.md5",
            "in": "vendor"
        }
    ],
    "git-version": "package_version"
}
